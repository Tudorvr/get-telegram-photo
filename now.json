{
  "version": 2,
  "name": "get-telegram-photo",
  "alias": ["get-telegram-photo"],
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": true
  },
  "public": true,
  "regions": ["all"],
  "routes": [
    {
      "src": "/.*",
      "headers": { "Access-Control-Allow-Origin": "*" }
    }
  ]
}
